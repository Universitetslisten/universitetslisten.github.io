{
  "name": "Universitetslisten",
  "version": "1.0.0",
  "description": "Universitetslisten nettside",
  "devDependencies": {
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^1.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "build-dev": "npm install && NODE_ENV=development webpack --config webpack.config.js",
    "jekyll-build": "bundle exec jekyll build --config _config_dev.yml",
    "jekyll-serve": "bundle exec jekyll serve --no-watch -l --config _config_dev.yml",
    "jekyll-serve-no-watch": "jekyll serve -l --config _config_dev.yml",
    "dev": "npm run build-dev && npm run jekyll-serve",
    "update-svg-colors": "python3 update_svg_colour.py"
  },
  "repository": {
    "type": "git",
